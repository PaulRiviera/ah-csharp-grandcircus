@{
    ViewData["Title"] = "Summary";

    var movie = ViewData["movie"] as Movie;
}

<h1>Movie @movie.Title was registered.</h1>
@Html.ActionLink("Go Home", "Index")
<br />
<h4>ID: @movie.ID</h4>
<h4>Title: @movie.Title</h4>
<h4>Genre: @movie.Genre</h4>
<h4>Year: @movie.Year</h4>

<h4>Actors</h4>
<ul>
    @{
        for (int i = 0; i < movie.Actors.Count; i++)
        {
            <li>@movie.Actors[i]</li>
        }
    }
</ul>

<h4>Directors</h4>
<ul>
    @{
        for (int i = 0; i < movie.Directors.Count; i++)
        {
            <li>@movie.Directors[i]</li>
        }
    }
</ul>
